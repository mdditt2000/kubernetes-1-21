{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema.json",
    "class": "AS3",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.6.0",
        "id": "GSLB_Sample",
        "DNS2": {
            "class": "Tenant",
            "CAFE_EXAMPLE": {
                "class": "Application",
                "template": "generic",
                "WIP2_CAFE_EXAMPLE_COM": {
                    "class": "GSLB_Domain",
                    "domainName": "cafe2.example.com",
                    "resourceRecordType": "A",
                    "poolLbMode": "topology",
                    "pools": [
                        {
                            "use": "CAFE2_EXAMPLE_COM_EAST"
                        },
                        {
                            "use": "CAFE2_EXAMPLE_COM_WEST"
                        }
                    ]
                },
                "CAFE2_EXAMPLE_COM_EAST": {
                    "class": "GSLB_Pool",
                    "enabled": true,
                    "resourceRecordType": "A",
                    "lbModePreferred": "round-robin",
                    "members": [
                        {
                            "ratio": 10,
                            "server": {
                                "use": "/Common/Shared/LTM_EAST_01"
                            },
                            "virtualServer": "0"
                        },
                        {
                            "ratio": 10,
                            "server": {
                                "use": "/Common/Shared/LTM_EAST_02"
                            },
                            "virtualServer": "0"
                        }
                    ],
                    "monitors": [
                        {
                            "use": "tea_http"
                        },
                        {
                            "use": "coffee_http"
                        }
                    ]
                },
                "CAFE2_EXAMPLE_COM_WEST": {
                    "class": "GSLB_Pool",
                    "enabled": true,
                    "resourceRecordType": "A",
                    "lbModePreferred": "round-robin",
                    "members": [
                        {
                            "ratio": 10,
                            "server": {
                                "use": "/Common/Shared/LTM_WEST_01"
                            },
                            "virtualServer": "0"
                        },
                        {
                            "ratio": 10,
                            "server": {
                                "use": "/Common/Shared/LTM_WEST_02"
                            },
                            "virtualServer": "0"
                        }
                    ],
                    "monitors": [
                        {
                            "use": "tea_http"
                        },
                        {
                            "use": "coffee_http"
                        }
                    ]
                },
                "tea_http": {
                    "class": "GSLB_Monitor",
                    "monitorType": "http",
                    "receive": "",
                    "send": "GET /tea HTTP/1.1/\r/\nHost: cafe.example.com/\r/\nConnection: Close/\r/\n/\r/\n"
                },
                "tea_https": {
                    "class": "GSLB_Monitor",
                    "monitorType": "https",
                    "receive": "",
                    "send": "GET /tea HTTP/1.1/\r/\nHost: cafe.example.com/\r/\nConnection: Close/\r/\n/\r/\n"
                },
                "coffee_http": {
                    "class": "GSLB_Monitor",
                    "monitorType": "http",
                    "receive": "",
                    "send": "GET /coffee HTTP/1.1/\r/\nHost: cafe.example.com/\r/\nConnection: Close/\r/\n/\r/\n"
                },
                "coffee_https": {
                    "class": "GSLB_Monitor",
                    "monitorType": "https",
                    "receive": "",
                    "send": "GET /coffee HTTP/1.1/\r/\nHost: cafe.example.com/\r/\nConnection: Close/\r/\n/\r/\n"
                }
            }
        }
    }
}